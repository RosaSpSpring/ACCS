<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="10dp"
    android:paddingRight="10dp"
    android:paddingBottom="10dp"
    android:paddingLeft="2dp"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/cloueitem_xibaomingcheng"
        style="@style/CloudItemItem"
        android:text="细胞名称"
        />

    <TextView
        android:id="@+id/cloueitem_chuandaishijian"
        style="@style/CloudItemItem"
        android:text="传代时间"
        />

    <TextView
        android:id="@+id/cloueitem_peiyangshijian"
        style="@style/CloudItemItem"
        android:text="培养时间"
        />

    <TextView
        android:id="@+id/cloueitem_xibaohuanyeshijian"
        style="@style/CloudItemItem"
        android:text="换液时间"
       />

    <ImageView
        android:id="@+id/cloueitem_tupian"
        style="@style/CloudItemItem"
        android:src="@drawable/pic"
        />
</LinearLayout>